{
  "name": "cloudyx-platform",
  "version": "1.0.0",
  "description": "Cloudyx - AI-powered SaaS security management platform for businesses",
  "private": true,
  "type": "module",
  "workspaces": [
    "Cloudyx-Platform/apps/backend",
    "Cloudyx-Platform/dashboard",
    "Cloudyx-Platform/packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces",
    "build": "cd dashboard && npm install && npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "npm run test --workspaces",
    "install:all": "npm install"
  },
  "keywords": [
    "cloudyx",
    "saas-security",
    "enterprise-security",
    "compliance",
    "ai",
    "security-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0"
  }
}
